<!DOCTYPE html>
<html>
  <head>
    <title>MusicApp</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <%= render "partials/errors" %>
    <%= render "partials/signout" %>
    <%= link_to "All Bands", bands_url if logged_in? %>
    <%= yield %>
  </body>
</html>
